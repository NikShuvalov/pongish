<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="@dimen/center_line_drawable_offset"
        android:top="@dimen/center_line_drawable_offset"
        android:bottom="@dimen/center_line_drawable_offset"
        android:right="@dimen/center_line_width">

        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>

            <stroke
                android:color="@color/centerLine"
                android:dashWidth="@dimen/center_line_dash_size"
                android:dashGap="@dimen/center_line_dash_gap"
                android:width="@dimen/center_line_width"/>
        </shape>
    </item>
</layer-list>
